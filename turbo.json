{
	"pipeline": {
		"build": {
			"dependsOn": [
				"^build",
				"$NODE_ENV",
				"$DB_CONNECTION_STRING",
				"$DATABASE_URL",
				"$FIREBASE_AUTH_EMULATOR_HOST",
				"$FIREBASE_STORAGE_EMULATOR_HOST",
				"$IS_TESTING",
				"$IS_DEV",
				"$NEXT_PUBLIC_API_PATH",
				"$NEXT_PUBLIC_IS_DEV",
				"$NEXT_PUBLIC_IS_TESTING",
				"$NEXT_PUBLIC_SENTRY_DSN",
				"$POSTGRES_DB",
				"$POSTGRES_PASSWORD",
				"$POSTGRES_USER",
				"$SENTRY_AUTH_TOKEN",
				"$SENTRY_DSN",
				"$SENTRY_ORG",
				"$SENTRY_PROJECT",
				"$STRIPE_WEBHOOK_SECRET",
				"$STRIPE_SECRET_KEY",
				"$STRIPE_PUBLISHABLE_KEY",
				"$SUBSCRIPTION_PRICE_ID",
				"$MONTHLY_TIP_PRICE_ID",
				"$TWITCH_CLIENT_ID",
				"$TWITCH_CLIENT_SECRET",
				"$TWITCH_SECRET",
				"$TWITCH_EXTENSION_SECRET"
			],
			"outputs": ["dist/**", ".next/**", "build/**"]
		},
		"test": {
			"dependsOn": ["^build"],
			"outputs": ["coverage/**"]
		},

		"lint": {
			"outputs": []
		},
		"dev": {
			"dependsOn": ["^build"],
			"cache": false
		},
		"clean": {
			"cache": false
		},
		"seed": {
			"cache": false
		},
		"start": {
			"cache": false
		},
		"type-check": {
			"outputs": []
		}
	}
}
