{
	"globalEnv": [
		"DATABASE_URL",
		"ENVIRONMENT",
		"FIREBASE_APP_ID",
		"FIREBASE_AUTH_DOMAIN",
		"FIREBASE_CLIENT_EMAIL",
		"FIREBASE_COOKIE_SECRET_CURRENT",
		"FIREBASE_COOKIE_SECRET_PREVIOUS",
		"FIREBASE_MEASUREMENT_ID",
		"FIREBASE_MESSAGING_SENDER_ID",
		"FIREBASE_PROJECT_ID",
		"GOOGLE_APPLICATION_CREDENTIALS",
		"IS_DEV",
		"IS_PREVIEW",
		"MONTHLY_TIP_PRICE_ID",
		"NEXT_PUBLIC_API_KEY",
		"NEXT_PUBLIC_ADS_DISABLED",
		"NEXT_PUBLIC_FIREBASE_DATABASE_URL",
		"NEXT_PUBLIC_GOOGLE_ANALYTICS",
		"NEXT_PUBLIC_IS_DEV",
		"NEXT_PUBLIC_SENTRY_DSN",
		"NEXT_PUBLIC_SOCKET_HOST",
		"NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY",
		"NODE_ENV",
		"PORT",
		"SENTRY_AUTH_TOKEN",
		"SENTRY_DSN",
		"SENTRY_ORG",
		"SENTRY_PROJECT",
		"STRIPE_SECRET_KEY",
		"STRIPE_WEBHOOK_SECRET",
		"SUBSCRIPTION_PRICE_ID",
		"TWITCH_CLIENT_ID",
		"TWITCH_CLIENT_SECRET",
		"VERCEL_ENV",
		"VERCEL_URL"
	],
  "pipeline": {
    "build": {
      "dependsOn": [
				"db:generate",
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "build/**"
      ]
    },
    "topo": {
      "dependsOn": [
        "db:generate",
        "^topo"
      ]
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "coverage/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": [
        "db:generate"
      ],
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "seed": {
      "dependsOn": [
        "db:generate"
      ],
      "cache": false
    },
    "start": {
      "cache": false
    },
    "type-check": {
      "dependsOn": [
				"db:generate",
        "^topo"
      ]
    }
  }
}
