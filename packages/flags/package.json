{
  "name": "@kittr/flags",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": "./src/index.ts"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./src/index.ts"
      ]
    }
  },
  "scripts": {
    "format": "prettier \"**/*.{ts,tsx,md,mdx,json}\" --check",
    "format:fix": "prettier \"**/*.{js,jsx,ts,tsx,md,mdx,json}\" --write --check",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@vercel/edge-config": "^0.1.9",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@kittr/lint": "workspace:*",
    "@kittr/prettier": "workspace:*",
    "@kittr/tsconfig": "workspace:*",
    "eslint": "^8.42.0"
  }
}
