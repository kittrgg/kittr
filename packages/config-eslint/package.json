{
  "name": "@kittr/lint",
  "version": "0.0.0",
  "type": "commonjs",
  "bin": {
    "eslint": "../node_modules/eslint/bin/eslint.js",
    "eslint-runner": "./runner/index.mjs"
  },
  "files": [
    "node.js",
    "next.js"
  ],
  "scripts": {
    "eslint": "node ./runner/index.mjs",
    "eslint:fix": "node ./runner/index.mjs --fix",
    "format": "prettier \"**/*.{ts,tsx,md,mdx,json}\" --check",
    "format:fix": "prettier \"**/*.{js,jsx,ts,tsx,md,mdx,json}\" --write --check",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@kittr/tsconfig": "workspace:*",
    "@next/eslint-plugin-next": "canary",
    "@rushstack/eslint-patch": "1.2.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vercel/style-guide": "^4.0.2",
    "eslint": "^8.40.0",
    "eslint-config-turbo": "^1.9.3",
    "globby": "^13.1.4",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@kittr/prettier": "workspace:*",
    "@types/eslint": "^8.37.0"
  }
}
