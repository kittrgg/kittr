{
  "name": "@kittr/ui",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "exports": {
    ".": "./src/components/index.ts",
    "./new": "./src/new/index.ts",
    "./icons": "./src/icons/index.ts",
    "./styles.css": "./dist/compiled.css"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./src/components/index.ts"
      ],
      "new": [
        "./src/new/index.ts"
      ],
      "icons": [
        "./src/icons/index.ts"
      ]
    }
  },
  "scripts": {
    "build": "tailwindcss -o dist/compiled.css --minify",
    "dev": "concurrently \"tailwindcss -o dist/compiled.css --minify --watch\"",
    "format": "prettier \"**/*.{ts,tsx,md,mdx,json}\" --check",
    "format:fix": "prettier \"**/*.{js,jsx,ts,tsx,md,mdx,json}\" --write --check",
    "lint": "eslint-runner",
    "lint:fix": "eslint-runner --fix",
    "test": "vitest run",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@mantine/core": "^5.10.2",
    "@mantine/dates": "^5.10.2",
    "@mantine/hooks": "^5.10.2",
    "@mantine/next": "^5.10.2",
    "@mantine/rte": "^5.10.2",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "lucide-icons-react": "^1.0.2",
    "react": "^18.2.0",
    "react-device-detect": "2.2.2",
    "react-dom": "^18.2.0",
    "simple-icons": "^8.13.0",
    "tailwind-merge": "^1.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.9",
    "@kittr/lint": "workspace:*",
    "@kittr/prettier": "workspace:*",
    "@kittr/tailwind": "workspace:*",
    "@kittr/tsconfig": "workspace:*",
    "@kittr/types": "workspace:*",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "2.0.0",
    "concurrently": "^8.0.1",
    "happy-dom": "^9.10.9",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vitest": "^0.31.0"
  }
}
