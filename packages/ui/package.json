{
	"name": "@kittr/ui",
	"version": "0.1.1",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"source": "src/index.tsx",
	"sideEffects": false,
	"private": true,
	"license": "MIT",
	"files": [
		"dist/**"
	],
	"scripts": {
		"test": "jest --coverage",
		"test:watch": "jest --watch --coverage",
		"build": "tsup src/index.tsx --format esm,cjs --dts --external react",
		"dev:currently-ignored": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
		"lint": "eslint \"**/*.tsx\" --fix",
		"clean": "rimraf ./.turbo  && rimraf ./dist && rimraf ./coverage && rimraf ./node_modules && echo UI Cleaned...",
		"type-check": "tsc --pretty --noEmit"
	},
	"devDependencies": {
		"@kittr/config": "*",
		"@testing-library/jest-dom": "5.16.2",
		"@testing-library/react": "12.1.3",
		"@types/react": "17.0.37",
		"@types/react-dom": "17.0.11",
		"babel-jest": "27.5.1",
		"eslint": "8.9.0",
		"eslint-config-next": "12.1.0",
		"eslint-plugin-jest": "26.1.1",
		"jest": "27.5.1",
		"ts-jest": "27.1.3",
		"tsup": "5.11.13",
		"typescript": "4.5.3"
	},
	"dependencies": {
		"@babel/core": "7.17.5",
		"@babel/plugin-transform-modules-commonjs": "7.16.8",
		"@kittr/logger": "*",
		"@kittr/types": "*",
		"@mantine/core": "3.6.9",
		"@mantine/dates": "3.6.9",
		"@mantine/hooks": "3.6.9",
		"@mantine/next": "3.6.9",
		"@mantine/rte": "3.6.9",
		"react": "17.0.2",
		"react-dom": "17.0.2"
	}
}
