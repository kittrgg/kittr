{
  "name": "@kittr/logger",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./node": "./dist/node/index.js",
    "./nextjs": "./dist/nextjs/index.js"
  },
  "typesVersions": {
    "*": {
      "nextjs": [
        "./src/nextjs/index.ts"
      ],
      "node": [
        "./src/node/index.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier \"**/*.{ts,tsx,md,mdx,json}\" --check",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@sentry/nextjs": "^7.55.2",
    "@sentry/node": "^7.55.2"
  },
  "devDependencies": {
    "@kittr/lint": "workspace:*",
    "@kittr/prettier": "workspace:*",
    "@kittr/tsconfig": "workspace:*",
    "@kittr/types": "workspace:*",
    "@sentry/types": "^7.34.0",
    "eslint": "^8.42.0",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3"
  }
}
