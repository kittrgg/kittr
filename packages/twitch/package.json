{
  "name": "@kittr/twitch",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": "./src/index.ts",
    "./types": "./src/types/index.ts"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./src/index.ts"
      ],
      "types": [
        "./src/types/index.ts"
      ]
    }
  },
  "scripts": {
    "format": "prettier \"**/*.{ts,tsx,md,mdx,json}\" --check",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@kittr/logger": "workspace:*",
    "@kittr/prisma": "workspace:*",
    "@kittr/types": "workspace:*",
    "@kittr/utils": "workspace:*"
  },
  "devDependencies": {
    "@kittr/lint": "workspace:*",
    "@kittr/tsconfig": "workspace:*",
    "eslint": "^8.42.0",
    "typescript": "^5.1.3"
  }
}
