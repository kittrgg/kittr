{
  "name": "kittr",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "clean": "find . -name node_modules -o -name .turbo -o -name .next -o -name dist -type d -prune | xargs rm -rf",
    "dev": "turbo dev --concurrency 100",
    "e2e": "turbo e2e --filter=playwright-web",
    "format": "turbo format format-root --continue",
    "format-root": "prettier \"**/*.{ts,tsx,md,mdx,json}\" \"!apps/**\" \"!packages/**\" --check",
    "format:fix": "turbo format format-root --continue -- --write",
    "lint": "turbo lint --continue -- --cache --cache-location 'node_modules/.cache/.eslintcache'",
    "lint-staged": "lint-staged",
    "lint:fix": "turbo lint --continue -- --fix",
    "prepare": "husky install",
    "start": "turbo start",
    "test": "turbo test --continue --filter=ui",
    "typecheck": "turbo typecheck --continue"
  },
  "devDependencies": {
    "@kittr/prettier": "workspace:*",
    "@vercel/style-guide": "4.0.2",
    "eslint": "^8.41.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "2.7.1",
    "prettier-plugin-packagejson": "^2.4.3",
    "shell-quote": "^1.8.1",
    "turbo": "canary"
  },
  "packageManager": "pnpm@8.2.0"
}
