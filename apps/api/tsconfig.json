{
  "extends": "@kittr/tsconfig/base.json",
  "compilerOptions": {
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
    "baseUrl": "./src",
    "outDir": "dist",
    "paths": {
      "@Jobs/*": ["./jobs/*"],
      "@Services/*": ["./services/*"],
      "@Types/*": ["./types/*"],
      "@Utils/*": ["./utils/*"]
    }
  },
  "ts-node": { "swc": true },
  "include": ["."],
  "exclude": ["node_modules", "dist", ".turbo"]
}
