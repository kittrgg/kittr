{
	"name": "api",
	"version": "0.0.0",
	"scripts": {
		"build": "rm -rf build && prisma generate && tsc -p tsconfig.json",
		"compile": "tsc && node ./build/src/index.js",
		"dev": "prisma migrate dev && nodemon -e ts --exec \"npm run compile\"",
		"start": "node ./build/src/index.js && prisma migrate deploy",
		"test": "prisma generate && jest --coverage",
		"lint": "eslint \"**/*.ts\" --ignore-pattern node_modules/",
		"clean": "rm -rf .turbo && rm -rf coverage && rm -rf build && rm -rf node_modules",
		"ci:test": "cross-env DATABASE_URL=test prisma generate && jest .*\\.unit\\.test\\.ts"
	},
	"_moduleAliases": {
		"@Root": "build/src",
		"@Utils": "build/src/utils",
		"@Libs": "build/src/libs",
		"@v1": "build/src/v1"
	},
	"prisma": {
		"seed": "ts-node prisma/seed.ts"
	},
	"dependencies": {
		"@faker-js/faker": "^5.5.3",
		"@prisma/client": "^3.8.1",
		"@sentry/node": "^6.17.3",
		"@sentry/tracing": "^6.16.1",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dotenv": "^10.0.0",
		"express": "^4.17.2",
		"firebase-admin": "^10.0.1",
		"joi": "^17.6.0",
		"module-alias": "^2.2.2",
		"nodemon": "^2.0.15",
		"prisma": "^3.7.0",
		"typescript": "^4.5.5"
	},
	"devDependencies": {
		"@babel/core": "^7.16.12",
		"@babel/plugin-transform-modules-commonjs": "^7.16.8",
		"@babel/preset-typescript": "^7.16.7",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/faker": "^6.6.9",
		"@types/jest": "^27.4.0",
		"@types/module-alias": "^2.0.1",
		"@types/node": "^17.0.8",
		"@types/supertest": "^2.0.11",
		"babel-jest": "^27.4.6",
		"jest": "^27.4.7",
		"supertest": "^6.2.2",
		"ts-jest": "^27.1.3",
		"ts-node": "^10.4.0",
		"tsconfig-paths": "^3.12.0"
	}
}
